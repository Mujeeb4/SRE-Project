{{if .dependencies}}
<div class="item">
	<div class="content">
		<div class="header"><strong>{{.title}}</strong></div>
		<div class="list">
			{{range $dependency, $version := .dependencies}}
				<div class="item">
					<i class="icon">{{svg "octicon-package-dependencies" 16 ""}}</i>
					<div class="content">
						<div class="header">{{$dependency}}</div>
						<div class="description">{{$version}}</div>
					</div>
				</div>
			{{end}}
		</div>
	</div>
</div>
{{end}}
