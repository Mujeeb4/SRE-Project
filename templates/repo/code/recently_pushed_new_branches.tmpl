{{range .RecentlyPushedNewBranches}}
	<div class="ui positive message gt-df gt-ac">
		<div class="gt-f1">
			{{$timeSince := TimeSince .UpdatedUnix.AsTime $.locale}}
			{{$.locale.Tr "repo.pulls.recently_pushed_new_branches" .Name $timeSince | Safe}}
		</div>
		<a aria-role="button" class="ui compact positive button gt-m-0" href="{{$.Repository.ComposeBranchCompareURL $.Repository.BaseRepo .Name}}">
			{{$.locale.Tr "repo.pulls.compare_changes"}}
		</a>
	</div>
{{end}}
