<div class="actions">
	<button type="button"{{/* To prevent accidental submission when clicking on no */}} class="ui red basic inverted cancel button">
		{{svg "octicon-x"}}
		{{.locale.Tr "modal.no"}}
	</button>
	<button class="ui green basic inverted ok button">
		{{svg "octicon-check"}}
		{{.locale.Tr "modal.yes"}}
	</button>
</div>
