{{template "base/head" .}}
<div role="main" aria-label="{{.Title}}" class="page-content {{if .IsRepo}}repository{{end}}">
	{{if .IsRepo}}{{template "repo/header" .}}{{end}}
	<div class="ui container">
		<div class="status-page-error">
			<div class="status-page-error-title">404 Not Found</div>
			<p class="status-page-error-detail">{{if .NotFoundPrompt}}{{.NotFoundPrompt}}{{else}}{{ctx.Locale.Tr "error404"}}{{end}}</p>
			{{if .NotFoundGoBackURL}}<a class="ui button tw-mr-0" href="{{.NotFoundGoBackURL}}">{{ctx.Locale.Tr "go_back"}}</a>{{end}}
		</div>
	</div>
</div>
{{template "base/footer" .}}
