<!DOCTYPE html>
<html>
	<head>
		<title>Links for {{.Package.Name}}</title>
	</head>
	<body>
		<h1>Links for {{.Package.Name}}</h1>
		{{range .Packages}}
			{{$p := .}}
			{{range .Files}}
				<a href="{{$.RegistryURL}}/files/{{$p.LowerName}}/{{$p.Version}}/{{.Name}}#sha256-{{.HashSHA256}}"{{if $p.Metadata.RequiresPython}} data-requires-python="{{$p.Metadata.RequiresPython}}"{{end}}>{{.Name}}</a><br/>
			{{end}}
		{{end}}
	</body>
</html>