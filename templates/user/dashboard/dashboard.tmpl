{{template "base/head" .}}
<div role="main" aria-label="{{.Title}}" class="page-content dashboard feeds">
	{{template "user/dashboard/navbar" .}}
	<div class="ui container">
		{{template "base/alert" .}}
		<div class="ui mobile reversed stackable grid">
			<div class="ui container ten wide column tw-text-center">
				{{template "user/heatmap" .}}
				{{if eq (len .Feeds) 0}}
					<div class="tw-pt-[60px] empty-placeholder">
						{{svg "octicon-rss" 48}}
						<h2>{{ctx.Locale.Tr "action.empty"}}</h2>
						<p>{{ctx.Locale.Tr "action.empty.desc" "/repo/create" "/repo/migrate" "/org/create"}}</p>
					</div>
				{{else}}
					{{template "user/dashboard/feeds" .}}
				{{end}}
			</div>
			{{template "user/dashboard/repolist" .}}
		</div>
	</div>
</div>
{{template "base/footer" .}}
