{{template "base/head" .}}
<div class="page-content user settings security">
	{{template "user/settings/navbar" .}}
	<div class="ui container">
		{{template "base/alert" .}}
		{{template "user/settings/security_twofa" .}}
		{{template "user/settings/security_u2f" .}}
		{{template "user/settings/security_accountlinks" .}}
		{{if .EnableOpenIDSignIn}}
		{{template "user/settings/security_openid" .}}
		{{end}}
	</div>
</div>

{{template "user/settings/token_modal" Dict "Title" "settings.twofa_enrolled" "Info" "settings.twofa_enrolled_info" "Token" .ScratchToken "i18n" .i18n}}

{{template "base/footer" .}}
