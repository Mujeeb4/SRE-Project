<div class="ui small modal" id="webauthn-error">
	<div class="header">{{.i18n.Tr "webauthn_error"}}</div>
	<div class="content">
		<div class="ui negative message">
			<div class="header">
			{{.i18n.Tr "webauthn_error"}}
			</div>
			<div class="hide" data-webauthn-error-msg="browser">{{.i18n.Tr "webauthn_unsupported_browser"}}</div>
			<div class="hide" data-webauthn-error-msg="1">{{.i18n.Tr "webauthn_error_1"}}</div>
			<div class="hide" data-webauthn-error-msg="2">{{.i18n.Tr "webauthn_error_2"}}</div>
			<div class="hide" data-webauthn-error-msg="3">{{.i18n.Tr "webauthn_error_3"}}</div>
			<div class="hide" data-webauthn-error-msg="4">{{.i18n.Tr "webauthn_error_4"}}</div>
			<div class="hide" data-webauthn-error-msg="5">{{.i18n.Tr "webauthn_error_5"}}</div>
			<div class="hide" data-webauthn-error-msg="0"></div>
		</div>
	</div>
	<div class="actions">
		<button onclick="window.location.reload()" class="success ui button hide webauthn_error_5">{{.i18n.Tr "webauthn_reload"}}</button>
		<div class="ui cancel button">{{.i18n.Tr "cancel"}}</div>
	</div>
</div>
